<h1>Listing publicacions</h1>

<table>
  <thead>
    <tr>
      <th>Titulo publicacion</th>
      <th>Descripcion publicacion</th>
      <th>Fecha publicacion</th>
      <th>Codigo usuario</th>
      <th>Codigo categoria</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @publicacions.each do |publicacion| %>
      <tr>
        <td><%= publicacion.titulo_publicacion %></td>
        <td><%= publicacion.descripcion_publicacion %></td>
        <td><%= publicacion.fecha_publicacion %></td>
        <td><%= publicacion.codigo_usuario %></td>
        <td><%= publicacion.codigo_categoria %></td>
        <td><%= link_to 'Show', publicacion %></td>
        <td><%= link_to 'Edit', edit_publicacion_path(publicacion) %></td>
        <td><%= link_to 'Destroy', publicacion, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Publicacion', new_publicacion_path %>
